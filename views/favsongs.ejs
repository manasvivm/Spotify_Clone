<!DOCTYPE html>
<html>

<head>
    <title> songs</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">

    <link rel="stylesheet" type="text/css" href="style_fav.css">
	
	<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@3.3.7/dist/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
	

	<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
  <script src="https://kit.fontawesome.com/eaaf0ba91e.js" crossorigin="anonymous"></script>
  
</head>

<body>
    <br>
    <br>
    <br>
    

               
				<br>
				<br>
				<h1 id="title">FAVOURITES DISPLAY PAGE</h1>
				
			
	
 <table class="table">
 			
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">SONG</th>
    </tr>
  </thead>
  <tbody>
  
  
  <%displaylist.forEach(item => {%>
  <div class='<%= item.song %>'  >
   <tr id>
      <th scope="row"><%= item._id %></th>
      <td ><%= item.song %></td>
      <form action="/removefav_song" method="POST"  id ="form" >
	 <td>
	  
		<li><input type="submit" id='<%= item.song %>' name='<%= item.song %>' class="song"  hidden="hidden" />
		<label for='<%= item.song %>'><i class="fa-solid fa-trash" id="img"></i></label>
		</li>
	  
	 </td>
	 </form>
	 
		
		
		
    </tr>
	</div>			
				<%})%>
   
		  
   
    
  </tbody>
</table>
          
</body>

</html>